{
  "name": "okr",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.1",
    "@opentelemetry/core": "^1.15.2",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.41.2",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.41.2",
    "@opentelemetry/resource-detector-alibaba-cloud": "^0.28.1",
    "@opentelemetry/resource-detector-aws": "^1.3.1",
    "@opentelemetry/resource-detector-container": "^0.3.1",
    "@opentelemetry/resource-detector-gcp": "^0.29.1",
    "@opentelemetry/resources": "^1.15.2",
    "@opentelemetry/sdk-metrics": "^1.15.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@prisma/client": "^5.0.0",
    "axios": "^1.4.0",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fp-ts": "^2.16.1",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.1",
    "kafkajs": "^2.2.4",
    "npm": "^9.8.1",
    "opentelemetry-instrumentation-kafkajs": "^0.39.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "shallow-equal-object": "^1.1.1",
    "uuidv4": "^6.2.13",
    "validator": "^13.9.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^5.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/prisma/",
      "<rootDir>/coverage/"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
